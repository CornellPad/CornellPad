<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:CornellPad"
             x:Class="CornellPad.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/FABrands.xaml" />
                <ResourceDictionary Source="Resources/Styles/FARegular.xaml" />
                <ResourceDictionary Source="Resources/Styles/FASolid.xaml" />
                <ResourceDictionary Source="Resources/Styles/Typographies.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
